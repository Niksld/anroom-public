config_version=5

[application]

config/name="Anroom"
config/description="First person non-euclidian puzzle game"
run/main_scene="res://src/levels/Lobby.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Anroom"
config/features=PackedStringArray("4.1", "Forward Plus")
run/max_fps=240
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://src/resources/icons/anroom_icons/icon_fullsize.png"
config/macos_native_icon="res://resources/icons/anroom_icons/icon.icns"
config/windows_native_icon="res://src/resources/icons/anroom_icons/icon.ico"

[debug]

shapes/navigation/geometry_face_color=Color(0.501961, 1, 1, 1)
shapes/navigation/geometry_edge_disabled_color=Color(0.921569, 0, 0, 1)
shapes/navigation/geometry_face_disabled_color=Color(1, 0, 0, 1)
shapes/navigation/link_connection_disabled_color=Color(0.94902, 0, 0, 1)
shapes/navigation/agent_path_point_size=16.0
shapes/collision/shape_color=Color(0.984314, 0, 0, 1)
shapes/collision/contact_color=Color(1, 0.2, 0.101961, 1)
shapes/paths/geometry_width=16.0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/qodot/plugin.cfg")

[filesystem]

import/blender/enabled=false

